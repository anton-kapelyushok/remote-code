 {
  "manifest_version": 2,
  "name": "Code Injector",
  "version": "0.1.3",
  "permissions": [ 
    "tabs",
    "<all_urls>"
  ],
  

  "icons": {
    "32": "favicon.ico",
    "64": "favicon.ico",
    "16": "favicon.ico",
    "128": "favicon.ico"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],

  "background": {
      "scripts": ["background.js"]
  }

    
}